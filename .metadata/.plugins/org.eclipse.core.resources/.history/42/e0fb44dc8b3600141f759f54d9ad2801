package programset2;

public class Controller {
	public String Name;
	private String Type;
	private String Core;
	
	/*Default constructor*/
	Controller(){
		Name = "Infinion";
		Type = "32_byte";
		Core = "Tricore";
		
	}
	
	Controller(String NameController){
		/*this() will call the default constructor and set the initialise the variables */
		this();
		Name = NameController;
	}
	
	Controller(String TypeController, String CoreController){
		Type = TypeController;
		Core = CoreController;
		
	}

}
